{
  "dockerComposeFile": ["docker-compose.yml"],
  "service": "rust_devcontainer",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "rust-lang.rust-analyzer",
        "vadimcn.vscode-lldb",
        "serayuzgur.crates",
        "JScearcy.rust-doc-viewer",
        "Swellaby.vscode-rust-test-adapter",
        "tamasfe.even-better-toml",
        "ritwickdey.LiveServer"
      ],
      "rust-analyzer.linkedProjects": [
        "app/Cargo.toml",
        "app/api/Cargo.toml",
        "app/domain/Cargo.toml",
        "app/app_service/Cargo.toml",
        "app/app_service/examples/user/Cargo.toml",
        "app/infra/migration/Cargo.toml",
        "app/infra/entity/Cargo.toml",
        "app/infra/repository/Cargo.toml",
        // "app/infra/repository/examples/user/Cargo.toml"
      ]
    }
  }
}
